pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                git 'https://github.com/sunrawat/jks.git'
                sh 'npm install'
                sh 'npm run build'
                sh 'mv dist/learn/* /var/www/html'
            }
        }
        stage('build') {
            steps {
                echo 'Hello World'
            }
        }
        stage('test') {
            steps {
                echo 'Hello World'
            }
        }
        stage('reelase') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
