pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                git 'https://github.com/sunrawat/jks.git'
            }
        }
        stage('build') {
            steps {
                echo 'Hello World build'
                sh 'npm install'
                sh 'npm run build'
            }
        }
        stage('test') {
            steps {
                echo 'Hello World test',
                sh 'npm run test'
            }
        }
        stage('reelase') {
            steps {
                echo 'Hello World - releasing'
                sh 'mv dist/learn/* /var/www/html'
            }
        }
    }
}
